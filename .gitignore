# Ignore bundler config.
/myapp/.bundle

# Ignore all logfiles and tempfiles.
/myapp/log/*
/myapp/tmp/*
!/myapp/log/.keep
!/myapp/tmp/.keep

# Ignore uploaded files in development
/myapp/storage/*
!/myapp/storage/.keep

/myapp/node_modules
/myapp/yarn-error.log

/myapp/public/assets
/myapp/.byebug_history

# Ignore master key for decrypting credentials and more.
/myapp/config/master.key

/myapp/vendor/bundle/*
/myapp/.env
